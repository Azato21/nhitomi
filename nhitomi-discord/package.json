{
  "name": "nhitomi-discord",
  "version": "1.0.0",
  "private": true,
  "main": "build/index.js",
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.13-4.3.1",
    "@types/config": "0.0.36",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "config": "^3.3.1",
    "discord.js": "^12.2.0",
    "eslint": "^7.0.0",
    "symlink-dir": "^4.0.3",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {
    "nhitomi-api": "file:nhitomi-api"
  },
  "scripts": {
    "postinstall": "node -e \"require('symlink-dir')('../locales', './locales')\"",
    "start": "tsc && node build/index.js",
    "build": "tsc",
    "genclient": "node ../scripts/genclient typescript-node"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "../eslint.json"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
}
