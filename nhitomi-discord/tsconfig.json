{
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "build",
    "sourceMap": true,
    "lib": [
      "esnext"
    ]
  }
}
