{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "program": "${workspaceFolder}\\build\\index.js",
      "autoAttachChildProcesses": true,
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
